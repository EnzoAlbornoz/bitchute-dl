{
    "env": {
        "commonjs": true,
        "es6": true,
		"node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
	],
	"plugins": ["prettier"],
	"root": true,
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
		"prettier/prettier": "error"
    }
}
